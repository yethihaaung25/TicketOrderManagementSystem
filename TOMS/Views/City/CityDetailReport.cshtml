

<h1>City Detail Report</h1>

@if (TempData["info"] != null)
{
    <div class="alert alert-warning">
        <span>@TempData["info"]</span>
    </div>
}

<form class="form-control" action="/city/CityDetailReport" method="post">
    <div class="form-group mb-2">
        <label for="staticEmail2" class="">Code</label>
        <input type="text" class="form-control" id="staticEmail2" name="code">
    </div>
    <div class="form-group mb-2">
        <label for="staticEmail2" class="">Name</label>
        <input type="text" class="form-control" id="staticEmail2" name="name">
    </div>
    <div class="form-group mb-2">
        <label for="staticEmail2" class="">Zip Code</label>
        <input type="text" class="form-control" id="staticEmail2" name="zipcode">
    </div>
    <button type="submit" class="btn btn-primary mb-2">Download</button>
</form>