

<h1>Passenger Detail Report</h1>

@if (TempData["info"] != null)
{
    <div class="alert alert-warning">
        <span>@TempData["info"]</span>
    </div>
}

<form class="form-control" action="/passenger/PassengerDetailReport" method="post">
    <div class="form-group mb-2">
        <label for="staticEmail2" class="">Name</label>
        <input type="text" class="form-control" id="staticEmail2" name="name">
    </div>
    <div class="form-group mb-2">
        <label for="staticEmail2" class="">NRC</label>
        <input type="text" class="form-control" id="staticEmail2" name="nrc">
    </div>
    <div class="form-group mb-2">
        <label for="staticEmail2" class="">Email</label>
        <input type="email" class="form-control" id="staticEmail2" name="email">
    </div>
    <button type="submit" class="btn btn-primary mb-2">Download</button>
</form>